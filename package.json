{
  "name": "submitit",
  "version": "1.0.0",
  "description": "A CLI tool that transforms deliverable packaging into a polished, intentional ritual",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "submitit": "./working-cli.js",
    "submitit-demo": "./simple-cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx watch src/working-cli.ts",
    "demo": "tsx src/simple-cli.ts",
    "preview": "tsx src/working-cli.ts preview",
    "test": "tsx src/test-real-functionality.ts",
    "test:staging": "tsx src/scripts/test-staging.ts",
    "lint": "eslint src --ext .ts,.tsx,.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'
  },
  "keywords": [
    "cli",
    "packaging",
    "deliverables",
    "submission",
    "portfolio",
    "astro",
    "ink",
    "yoga",
    "terminal"
  ],
  "author": "Cameron Brooks",
  "license": "MIT",
  "dependencies": {
    "@astrojs/node": "^9.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@inquirer/confirm": "^4.0.5",
    "@inquirer/input": "^3.1.0",
    "@inquirer/select": "^5.2.1",
    "@inkjs/ui": "^2.0.0",
    "@types/archiver": "^6.0.3",
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/globby": "^13.2.3",
    "@types/inquirer": "^9.0.7",
    "@types/lru-cache": "^9.1.0",
    "@types/node": "^20.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^18.2.18",
    "@types/yoga-layout": "^1.9.2",
    "archiver": "^7.0.1",
    "astro": "^5.11.0",
    "chalk": "^5.4.1",
    "chalk-animation": "^2.0.3",
    "commander": "^12.1.0",
    "execa": "^9.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "ink": "^6.0.1",
    "ink-spinner": "^5.0.0",
    "lru-cache": "^11.0.0",
    "mime-types": "^2.1.35",
    "ora": "^8.0.1",
    "p-retry": "^6.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "wait-port": "^1.0.4",
    "yaml": "^2.4.5",
    "yoga-layout": "^3.2.1",
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronbrooks/submitit.git"
  },
  "bugs": {
    "url": "https://github.com/cameronbrooks/submitit/issues"
  },
  "homepage": "https://github.com/cameronbrooks/submitit#readme"
}
